<h1> Login </h1>
<div class="sign-in-form">
  <%= form_tag sessions_path do|f| %>
  <div class=”field”>
    <%= label_tag :email%>
    <%= text_field_tag :email %>
  </div>
  <br>
  <div class=”field”>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <br>
  <div class=”actions”>
    <%= submit_tag "Login" %>
  </div>
<% end %>
</div>

<div>
  <p> If you don't have an account you can register here: <%= link_to 'Register', new_user_path %> </p> 
